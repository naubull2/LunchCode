{
  "run": [
    {
      "input": { "nums": [2, 7, 11, 15], "target": 9 },
      "expected": [0, 1],
      "description": "Basic example with positive numbers"
    },
    {
      "input": { "nums": [3, 2, 4], "target": 6 },
      "expected": [1, 2],
      "description": "Example with target as sum of middle elements"
    },
    {
      "input": { "nums": [3, 3], "target": 6 },
      "expected": [0, 1],
      "description": "Example with duplicate elements"
    }
  ],
  "submit": [
    {
      "input": { "nums": [1, 2, 3, 4, 5], "target": 9 },
      "expected": [3, 4],
      "description": "Larger array with positive numbers"
    },
    {
      "input": { "nums": [-1, -2, -3, -4, -5], "target": -8 },
      "expected": [2, 4],
      "description": "Array with negative numbers"
    },
    {
      "input": { "nums": [1000000000, -1000000000], "target": 0 },
      "expected": [0, 1],
      "description": "Edge case with large numbers"
    }
  ]
}
